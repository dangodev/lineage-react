{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["emotion", "prettier", "import", "react", "react-hooks"],
  "rules": {
    "camelcase": "off",
    "curly": "error",
    "no-irregular-whitespace": "off"
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  }
}
